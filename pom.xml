                                GPL2 w/ CPE|
                                GPL-2.0-with-classpath-exception
                            </licenseMerge>
                            <licenseMerge>
                                LGPL 2.1|
                                LGPL, version 2.1
                            </licenseMerge>
                            <licenseMerge>
                                MIT License|
                                MIT license|
                                The MIT License (MIT)
                            </licenseMerge>
                            <licenseMerge>
                                BSD-3-Clause|
                                Modified BSD
                            </licenseMerge>
                            <licenseMerge>
                                GNU Lesser General Public License, version 2.1|
                                LGPL 2.1
                            </licenseMerge>
                        </licenseMerges>
                    </configuration>
                    <executions>
                        <execution>
                            <id>create-license-list</id>
                            <goals>
                                <goal>aggregate-add-third-party</goal>
                            </goals>
                            <phase>generate-resources</phase>
                        </execution>
                    </executions>
                </plugin>
            </plugins>
        </pluginManagement>

