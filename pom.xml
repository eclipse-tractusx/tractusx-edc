    <profiles>
        <profile>
            <id>generate-notice</id>
            <build>
                <plugins>
                    <plugin>
                        <!--
                            mvn notice:generate -Pgenerate-notice
                        -->
                        <groupId>org.jasig.maven</groupId>
                        <artifactId>maven-notice-plugin</artifactId>
                        <version>1.1.0</version>
                        <dependencies>
                            <dependency>
                                <groupId>jakarta.xml.bind</groupId>
                                <artifactId>jakarta.xml.bind-api</artifactId>
                                <version>4.0.0</version>
                            </dependency>
                            <dependency>
                                <groupId>org.glassfish.jaxb</groupId>
                                <artifactId>jaxb-runtime</artifactId>
                                <version>2.3.6</version>
                            </dependency>
                        </dependencies>
                        <configuration>
                            <noticeTemplate>${project.basedir}/misc/NOTICE.md.template</noticeTemplate>
                            <generateChildNotices>false</generateChildNotices>
                            <noticeMessage>* {0} under {1}</noticeMessage>
                            <fileName>NOTICE.md</fileName>
                            <noticeTemplatePlaceholder>@{GENERATED_NOTICES}</noticeTemplatePlaceholder>
                            <licenseMapping>
                                <param>${project.basedir}/misc/license-mappings.xml</param>
                            </licenseMapping>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
